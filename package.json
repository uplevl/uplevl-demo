{
  "name": "uplevl-demo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "next dev --turbopack",
    "inngest": "pnpm dlx inngest-cli@latest dev",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome check",
    "lint:staged": "lint-staged -q",
    "typecheck": "tsc",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "DB_MIGRATING=true bun run src/server/database/migrate.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "pr-tweet": "tsx scripts/pr-tweet/index.ts"
  },
  "dependencies": {
    "@hono/zod-validator": "0.7.3",
    "@hookform/resolvers": "^5.2.1",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^10.11.0",
    "@supabase/supabase-js": "^2.57.4",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.87.4",
    "@upstash/redis": "^1.35.3",
    "ai": "5.0.44",
    "axios": "^1.12.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "hono": "4.9.7",
    "immer": "^10.1.3",
    "import-in-the-middle": "^1.14.2",
    "inngest": "3.40.3",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "p-map": "^7.0.3",
    "postgres": "^3.4.7",
    "posthog-js": "^1.266.0",
    "posthog-node": "^5.8.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "require-in-the-middle": "^7.5.2",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-safe-area": "^1.1.0",
    "zod": "4.1.9",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "concurrently": "^9.2.1",
    "cosmiconfig": "^9.0.0",
    "drizzle-kit": "^0.31.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "netlify-plugin-inngest": "^1.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "5.9.2"
  },
  "lint-staged": {
    "**/*": [
      "biome lint --fix",
      "biome format --write"
    ]
  },
  "prepare-commit-msg": {
    "messagePattern": "[$J] $M",
    "jiraTicketPattern": "(CU-[a-z0-9]+)",
    "ignoreBranchesMissingTickets": true
  },
  "engines": {
    "node": ">=22.18.0",
    "pnpm": ">=9.14.2"
  },
  "packageManager": "pnpm@9.14.2"
}
